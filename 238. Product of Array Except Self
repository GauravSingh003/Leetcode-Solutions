// The product of any prefix or suffix of nums is guaranteed to fit in a 32-bit integer.
 var product = function(nums){
let res = [];
let start = 1;
for(let i =0; i<nums.length; i++){
res.push(start);
start = start*2nums[i];
}
let start2 = 1;
for(let i = nums.length-1; i>=0; i++){
res[i] = start2*res[i];
start*2 = start*2nums[2]
}
return res;
 }
